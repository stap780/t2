<%= turbo_frame_tag dom_id(incase_dubl), class: "grid grid-cols-[170px_1fr_150px_150px_150px_150px_100px] gap-2 px-3 py-2 border-b hover:bg-gray-50 items-center" do %>
  <div class="text-sm"><%= incase_dubl.unumber %></div>
  <div class="text-sm"><%= incase_dubl.company&.short_title %></div>
  <div class="text-sm">
    <% if incase_dubl.existing_incase %>
      <%= incase_dubl.existing_incase.date&.strftime('%d.%m.%Y') %>
    <% else %>
      <span class="text-gray-400">-</span>
    <% end %>
  </div>
  <div class="text-sm"><%= incase_dubl.date&.strftime('%d.%m.%Y') %></div>
  <div class="text-sm">
    <% if incase_dubl.has_differences? %>
      <span class="px-2 py-1 text-xs font-medium rounded bg-yellow-100 text-yellow-800">
        <%= t('.has_differences') %>
      </span>
    <% else %>
      <span class="text-gray-400">-</span>
    <% end %>
  </div>
  <div class="text-sm text-gray-600">
    <%= incase_dubl.incase_import.created_at.strftime('%d.%m.%Y') %>
  </div>
  <div class="flex justify-end space-x-2">
    <%= link_to_show incase_dubl_path(incase_dubl), data: { turbo: false } %>
    <%= link_to_delete incase_dubl_path(incase_dubl), title: 'delete', data: { turbo_method: :delete, turbo_confirm: 'Are you sure?'} %>
  </div>
<% end %>

